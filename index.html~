function showhide(obj)
{
	var  obj1=document.getElementById(obj);
	//alert(obj1.style.display);
	if (obj1.style.display == 'block')
	{ obj1.style.display='none'; }
	else
	{ obj1.style.display='block';}
	return(false);
}
